<!DOCTYPE html>
<html lang="vi">
	<head>
		<meta charset="utf-8">
		<meta http-equiv="X-UA-Compatible" content="IE=edge">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<title>{% block title_page %}Chào mừng{% endblock %}</title>
		<!-- Bootstrap CSS -->
		<link rel="stylesheet" href="/assets/bower_components/bootstrap/dist/css/bootstrap.min.css">
		<link rel="stylesheet" href="/assets/bower_components/font-awesome/css/font-awesome.min.css">

		{% block custom_css %}{% endblock %}
		<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
		<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
		<!--[if lt IE 9]>
			<script src="https://oss.maxcdn.com/libs/html5shiv/3.7.2/html5shiv.min.js"></script>
			<script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
		<![endif]-->
	</head>
	<body>
	<div class="container">

		{# {% embed "_includes/navbar.twig" %}{% endembed %} #}

		{% include '_includes/navbar.twig'%}
		{% if session.alert_error %}
			<div class="alert alert-danger">
				<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
				<strong>Error !</strong> {{session.alert_error}}
			</div>
		{% endif %}
		{% if session.alert_success %}
			<div class="alert alert-success">
				<button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
				<strong>Success</strong> {{session.alert_success}}
			</div>
		{% endif %}
		{% block content_page %}{% endblock %}

	</div>

		<!-- jQuery -->
		<script src="/assets/bower_components/jquery/dist/jquery.min.js"></script>
		<!-- Bootstrap JavaScript -->
		<script src="/assets/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
		<!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
		{% block custom_js %}{% endblock %}
	</body>
</html>